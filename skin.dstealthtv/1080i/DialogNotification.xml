<?xml version="1.0" encoding="UTF-8"?>
<!-- DSTV Changes! -->
<!-- DSTV: whole file. -->
<window>
	<zorder>3</zorder>
	<animation type="WindowOpen" reversible="false">
		<effect type="slide" start="0,-160" tween="cubic" easing="inout" time="720" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="slide" end="0,-160" tween="cubic" easing="inout" time="720" />
	</animation>
	<controls>
		<control type="group">
			<depth>DepthMax</depth>
			<top>0</top>
			<left>0</left>
			<right>0</right>
			<visible>String.IsEmpty(Window(Home).Property(script.cinemavision.running))</visible>
			<control type="image">
				<left>0</left>
				<top>0</top>
				<height>160</height>
				<texture>dialogs/dialog-bg-nobo.png</texture>
				<bordertexture border="21" infill="false">overlays/shadow.png</bordertexture>
				<bordersize>20</bordersize>
			</control>
			<control type="image" id="400">
				<description>avatar</description>
				<left>25</left>
				<top>25</top>
				<width>110</width>
				<height>110</height>
				<aspectratio>stretch</aspectratio>
				<texture colordiffuse="selected">defaulticoninfo.png</texture>
			</control>
			<control type="image" id="400">
				<description>avatar</description>
				<right>25</right>
				<top>25</top>
				<width>110</width>
				<height>110</height>
				<aspectratio>stretch</aspectratio>
				<texture colordiffuse="selected">defaulticoninfo.png</texture>
			</control>
			<control type="fadelabel" id="401">
				<description>Line 1 Label</description>
				<left>150</left>
				<right>150</right>
				<top>40</top>
				<height>18</height>
				<font>font37</font>
				<textcolor>selected</textcolor>
				<align>center</align>
				<aligny>center</aligny>
				<scrollout>false</scrollout>
				<pauseatend>2000</pauseatend>
			</control>
			<control type="fadelabel" id="402">
				<description>Line 2 Label</description>
				<left>151</left>
				<right>151</right>
				<top>68</top>
				<height>60</height>
				<font>font27_narrow</font>
				<align>center</align>
				<aligny>center</aligny>
				<scrollout>false</scrollout>
				<pauseatend>2000</pauseatend>
				<scrollspeed>25</scrollspeed>
			</control>
		</control>
	</controls>
</window>
